{
    "env": {
        "browser": true,
        "es2021": true,
        "jest":true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": "off",
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "no-unused-vars": "off",
        "react/function-component-definition": "off",
        "indend": "off"
    }
}
